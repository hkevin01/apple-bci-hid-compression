{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "## 🧰 Maintenance",
      "labels": ["chore", "documentation"]
    },
    {
      "title": "## 🛡️ Security",
      "labels": ["security"]
    }
  ],
  "ignore_labels": [
    "duplicate",
    "invalid",
    "question",
    "wontfix"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n## 📦 Dependencies\n\n${{CHANGES}}\n",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365
}
